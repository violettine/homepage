<h1> Your Youtube results: </h1>

<div id='search_yt_form'>
  <%= form_for(:search, url: api_youtube_request_path) do |f| %>
    <%= f.text_field :term %>
    <%= f.submit 'Search in Youtube', id: 'commit_search'  %>
  <% end %>
</div>

<% @resulting_videos.each do |movie| %>
  <div id='short_movie'>
     <iframe src=<%="https://www.youtube.com/embed/#{movie}"%> frameborder="0" allowfullscreen></iframe>
  </div>
<%end%>
